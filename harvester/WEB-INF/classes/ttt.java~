class ttt
{
   public static void main ( String [] args ) throws Exception
   {
      String s = "dsadsadsa dsadsa<    \n/ \r OAI-PMH >\n\r\t";
      if (s.matches ("(?sm).*OAI-PMH\\s*>\\s*$"))
         System.out.println("y");
      else     
         System.out.println("n"+s);
   }
}
